{
	"name": "omuso",
	"version": "1.0.2",
	"description": "Converts Markdown to a structured JSON format.",
	"module": "dist/index.js",
	"types": "dist/types/index.d.ts",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/marcmarine/omuso.git"
	},
	"bugs": "https://github.com/marcmarine/omuso/issues/new",
	"author": "Marc Marin√© <shenobi@gmail.com>",
	"license": "MIT",
	"scripts": {
		"build:types": "tsc ./src/index.ts --declaration --emitDeclarationOnly --outdir dist/types",
		"build": "bun build ./src/index.ts --outdir ./dist --target node --minify && bun run build:types"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/bun": "latest",
		"semantic-release": "^24.2.9"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"markdown",
		"md",
		"json",
		"parser",
		"syntax",
		"tree"
	]
}
